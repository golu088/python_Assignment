    "# for persistent states\n",
    "def counter():\n",
    "    # value of count is present here \n",
    "    count=0\n",
    "    def increment():\n",
    "        nonlocal count\n",
    "        count+=1\n",
    "        print(count)\n",
    "        \n",
    "\n",
    "    return increment \n",
    "\n",
    "c1 = counter()\n",
    "c1()\n",
    "c1()\n",
    "c2=counter()\n",
    "c2()\n",
    "c1()"
   ]
